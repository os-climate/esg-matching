{
  "file_processing": {
    "file_path": "../../tests/data/notebook/demo/test_ds2.csv",
    "file_extension_pattern": "*.csv",
    "filename_pattern": "test_target2",
    "encoding": "utf-8",
    "separator": ";",
    "read_mode": "bulk-pd"
  },
  "data_source": {
    "name": "ds_tgt2",
    "table_name": "esg_match_tgt2",
    "create_table": "True",
    "if_table_exists": "drop",
    "primary_keys": ["id_ds2"],
    "attributes": {
      "ID":  ["id_ds2","int"],
      "ISIN":  ["isin_ds2", "str"],
      "LEI":  ["lei_ds2", "str"],
      "SEDOL":  ["sedol_ds2", "str"],
      "COMPANY_NAME":  ["name_ds2", "str"],
      "COUNTRY": ["country_ds2", "str"]
    },
    "matching_role": "target",
    "matching_alias": {
      "ISIN":  "isin_ds2",
      "LEI":  "lei_ds2",
      "SEDOL":  "sedol_ds2",
      "COMPANY": "name_ds2",
      "COUNTRY": "country_ds2"
    },
    "map_to_matching": {
      "tgt_id":  "id_ds2",
      "isin":  "isin_ds2",
      "lei":  "lei_ds2",
      "sedol":  "sedol_ds2",
      "tgt_company": "name_ds2",
      "tgt_country": "country_ds2"
    }
  },
  "matching_policy": {
    "matching_with_ref1": {
      "dfm": {
        "lei":  ["LEI"]
      } ,
      "drm": {
        "name+country":  ["COMPANY", "COUNTRY"]
      },
      "irm": {
        "lei":  ["LEI"],
        "isin":  ["ISIN"],
        "name+country":  ["COMPANY", "COUNTRY"]
      }
    }
  }
}
